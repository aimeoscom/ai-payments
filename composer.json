{
	"name": "aimeos/ai-payments",
	"description": "Payment extension for Aimeos e-commerce solutions",
	"keywords": ["aimeos", "payment", "shop", "e-commerce", "omnipay", "stripe", "mollie", "authorize", "cardsave", "sagepay"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-payments",
		"issues": "https://github.com/aimeos/ai-payments/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.1",
		"aimeos/aimeos-core": "2025.04.*",
		"league/omnipay": "~3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~10.0||~11.0",
		"omnipay/dummy": "~3.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
