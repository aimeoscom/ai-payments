{
	"name": "aimeos/ai-payments",
	"description": "Payment extension for Aimeos e-commerce solutions",
	"keywords": ["aimeos", "payment", "shop", "e-commerce", "omnipay", "stripe", "mollie", "authorize", "cardsave", "sagepay"],
	"homepage": "https://aimeos.org/",
	"type": "aimeos-extension",
	"license": "LGPL-3.0-or-later",
	"support": {
		"source": "https://github.com/aimeos/ai-payments",
		"issues": "https://github.com/aimeos/ai-payments/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"aimeos/aimeos-core": "dev-master",
		"omnipay/common": "~3.0",
		"php-http/httplug": "~2.0",
		"php-http/curl-client": "~2.0",
		"php-http/message-factory": "~1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~9.0||~10.0",
		"guzzlehttp/psr7": "~2.5",
		"omnipay/dummy": "~3.0"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": true
		}
	}
}
