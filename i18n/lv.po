#, php-format
msgid "Method \"%1$s\" for provider not available"
msgstr "Metode \"%1$s\" pakalpojumu sniedzējam nav pieejama"

#, php-format
msgid "No Stripe customer data available for customer ID \"%1$s\""
msgstr "Par klienta ID \"%1$s\" nav pieejami Stripe klienta dati"

#, php-format
msgid "No Stripe payment method available for customer ID \"%1$s\""
msgstr "Klientam ar ID \"%1$s\" nav pieejama neviena Stripe maksājumu metode."

#, php-format
msgid "No payment token available for customer ID \"%1$s\""
msgstr "Klienta ID \"%1$s\" nav pieejams neviens maksājuma žetons."

#, php-format
msgid "No reoccurring payment data available for customer ID \"%1$s\""
msgstr "Par klienta ID \"%1$s\" nav pieejami atkārtotu maksājumu dati."

#, php-format
msgid "Order %1$s"
msgstr "Pasūtījums %1$s"

msgid "PayPalPlus approval URL not available"
msgstr "PayPalPlus apstiprināšanas URL nav pieejams"

msgid "PayPalPlus requires the country ID of the user"
msgstr "PayPalPlus pieprasa lietotāja valsts ID"

#, php-format
msgid "Token based payment failed with code \"%1$s\" and message \"%2$s\": %3$s"
msgstr ""
"Uz žetonu balstīts maksājums neizdevās ar kodu \"%1$s\" un ziņojumu "
"\"%2$s\": %3$s"

#, php-format
msgid "Token based payment failed: %1$s"
msgstr "Uz žetonu balstīts maksājums neizdevās: %1$s"

#, php-format
msgid "Token based payment incomplete: %1$s"
msgstr "Nepilnīgs uz žetoniem balstīts maksājums: %1$s"

#, php-format
msgid "Unexpected redirect: %1$s"
msgstr "Negaidīts pāradresējums: %1$s"
