#, php-format
msgid "Method \"%1$s\" for provider not available"
msgstr "O método \"%1$s\" para o provedor não está disponível"

#, php-format
msgid "No Stripe customer data available for customer ID \"%1$s\""
msgstr "Nenhum dado de cliente do Stripe disponível para o ID de cliente \"%1$s\""

#, php-format
msgid "No Stripe payment method available for customer ID \"%1$s\""
msgstr "Método de pagamento Stripe não disponível para o ID de cliente \"%1$s\""

#, php-format
msgid "No payment token available for customer ID \"%1$s\""
msgstr "Nenhum token de pagamento disponível para o ID de cliente \"%1$s\""

#, php-format
msgid "No reoccurring payment data available for customer ID \"%1$s\""
msgstr ""
"Nenhum dado de pagamento recorrente disponível para o ID de cliente \"%1$s\""

#, php-format
msgid "Order %1$s"
msgstr "Pedido %1$s"

msgid "PayPalPlus approval URL not available"
msgstr "URL de aprovação do PayPalPlus não disponível"

msgid "PayPalPlus requires the country ID of the user"
msgstr "O PayPalPlus exige o ID do país do usuário"

#, php-format
msgid "Token based payment failed with code \"%1$s\" and message \"%2$s\": %3$s"
msgstr ""
"O pagamento baseado em token falhou com o código \"%1$s\" e a mensagem "
"\"%2$s\": %3$s"

#, php-format
msgid "Token based payment failed: %1$s"
msgstr "Falha no pagamento baseado em token: %1$s"

#, php-format
msgid "Token based payment incomplete: %1$s"
msgstr "Pagamento baseado em token incompleto: %1$s"

#, php-format
msgid "Unexpected redirect: %1$s"
msgstr "Redirecionamento inesperado: %1$s"
