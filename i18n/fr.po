#, php-format
msgid "Method \"%1$s\" for provider not available"
msgstr "Méthode \"%1$s\" pour fournisseur non disponible"

#, php-format
msgid "No Stripe customer data available for customer ID \"%1$s\""
msgstr "Aucune donnée client Stripe disponible pour l'ID client \"%1$s\""

#, php-format
msgid "No Stripe payment method available for customer ID \"%1$s\""
msgstr ""
"Aucune méthode de paiement Stripe n'est disponible pour l'ID du client "
"\"%1$s\""

#, php-format
msgid "No payment token available for customer ID \"%1$s\""
msgstr "Aucun jeton de paiement disponible pour l'ID client \"%1$s\""

#, php-format
msgid "No reoccurring payment data available for customer ID \"%1$s\""
msgstr "Aucune donnée de paiement récurrent disponible pour l'ID client \"%1$s\""

#, php-format
msgid "Order %1$s"
msgstr "Ordre %1$s"

msgid "PayPalPlus approval URL not available"
msgstr "L'URL d'approbation de PayPalPlus n'est pas disponible"

msgid "PayPalPlus requires the country ID of the user"
msgstr "PayPalPlus demande l'identifiant du pays de l'utilisateur."

#, php-format
msgid "Token based payment failed with code \"%1$s\" and message \"%2$s\": %3$s"
msgstr ""
"Le paiement par jeton a échoué avec le code \"%1$s\" et le message \"%2$s\" "
": %3$s"

#, php-format
msgid "Token based payment failed: %1$s"
msgstr "Le paiement basé sur un jeton a échoué:%1$s"

#, php-format
msgid "Token based payment incomplete: %1$s"
msgstr "Paiement par jeton incomplet : %1$s"

#, php-format
msgid "Unexpected redirect: %1$s"
msgstr "Redirection inattendue: %1$s"
