#, php-format
msgid "Method \"%1$s\" for provider not available"
msgstr "\"%1$s\" módszer a szolgáltatóhoz nem elérhető"

#, php-format
msgid "No Stripe customer data available for customer ID \"%1$s\""
msgstr ""
"A \"%1$s\" ügyfél azonosítóhoz nem állnak rendelkezésre Stripe ügyféladatok."

#, php-format
msgid "No Stripe payment method available for customer ID \"%1$s\""
msgstr "Nincs elérhető Stripe fizetési mód a \"%1$s\" ügyfél azonosítóhoz."

#, php-format
msgid "No payment token available for customer ID \"%1$s\""
msgstr "Nincs elérhető fizetési token a \"%1$s\" ügyfél azonosítóhoz."

#, php-format
msgid "No reoccurring payment data available for customer ID \"%1$s\""
msgstr ""
"A \"%1$s\" ügyfél azonosítóhoz nem állnak rendelkezésre ismétlődő fizetési "
"adatok."

#, php-format
msgid "Order %1$s"
msgstr "Megrendelés %1$s"

msgid "PayPalPlus approval URL not available"
msgstr "PayPalPlus jóváhagyási URL nem elérhető"

msgid "PayPalPlus requires the country ID of the user"
msgstr "A PayPalPlus megköveteli a felhasználó országazonosítóját"

#, php-format
msgid "Token based payment failed with code \"%1$s\" and message \"%2$s\": %3$s"
msgstr ""
"A token alapú fizetés sikertelen volt \"%1$s\" kóddal és \"%2$s\" üzenettel:"
" %3$s"

#, php-format
msgid "Token based payment failed: %1$s"
msgstr "Token alapú fizetés sikertelen: %1$s"

#, php-format
msgid "Token based payment incomplete: %1$s"
msgstr "Token alapú fizetés nem teljes: %1$s"

#, php-format
msgid "Unexpected redirect: %1$s"
msgstr "Váratlan átirányítás: %1$s"
