#, php-format
msgid "Method \"%1$s\" for provider not available"
msgstr "Sağlayıcı için yöntem \"%1$s\" mevcut değil"

#, php-format
msgid "No Stripe customer data available for customer ID \"%1$s\""
msgstr "\"%1$s\" müşteri kimliği için hiçbir Stripe müşteri bilgisi mevcut değil"

#, php-format
msgid "No Stripe payment method available for customer ID \"%1$s\""
msgstr "\"%1$s\" müşteri kimliği için hiçbir Stripe ödeme yöntemi mevcut değil"

#, php-format
msgid "No payment token available for customer ID \"%1$s\""
msgstr "\"%1$s\" müşteri kimliği için herhangi bir ödeme belirteci yok"

#, php-format
msgid "No reoccurring payment data available for customer ID \"%1$s\""
msgstr "\"%1$s\" müşteri kimliği için herhangi bir yinelenen ödeme verisi yok"

#, php-format
msgid "Order %1$s"
msgstr "Sipariş %1$s"

msgid "PayPalPlus approval URL not available"
msgstr "PayPalPlus onay URL'si mevcut değil"

msgid "PayPalPlus requires the country ID of the user"
msgstr "PayPal Plus, kullanıcının ülke kimlik numarasını (ID) gerektirir"

#, php-format
msgid "Token based payment failed with code \"%1$s\" and message \"%2$s\": %3$s"
msgstr ""
"Token tabanlı ödeme \"%1$s\" koduyla ve \"%2$s\" mesajıyla başarısız oldu: "
"%3$s"

#, php-format
msgid "Token based payment failed: %1$s"
msgstr "Token-bazlı ödeme başarısız oldu: %1$s"

#, php-format
msgid "Token based payment incomplete: %1$s"
msgstr "Token tabanlı ödeme tamamlanmadı: %1$s"

#, php-format
msgid "Unexpected redirect: %1$s"
msgstr "Beklenmeyen yönlendirme: %1$s"
